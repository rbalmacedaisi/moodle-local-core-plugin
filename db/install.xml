>
  <TABLES>
    <TABLE NAME="gmk_contract" COMMENT="This table contains the list of contracts.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="The ID of the user related to the contract."/>
        <FIELD NAME="lpid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="The ID of the learning plan."/>
        <FIELD NAME="type" TYPE="char" LENGTH="255" NOTNULL="false" DEFAULT="individual" SEQUENCE="false" COMMENT="The type of the contract: individual = &quot;Contrato individual&quot;; institutional = &quot;Contrato institucional&quot;"/>
        <FIELD NAME="code" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="This internal code is automatically generated when the contract is fully approved."/>
        <FIELD NAME="status" TYPE="int" LENGTH="1" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="This field indicates what is the status of the contract; let's use a small number to manage this concept of status."/>
        <FIELD NAME="usermodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="usermodified" TYPE="foreign" FIELDS="usermodified" REFTABLE="user" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="gmk_tuition" COMMENT="Table to manage the tuitions of the students">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="amount" TYPE="number" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" DECIMALS="3" COMMENT="The amount payed by the user."/>
        <FIELD NAME="paymentmethod" TYPE="char" LENGTH="500" NOTNULL="false" SEQUENCE="false" COMMENT="The payment method used by the user."/>
        <FIELD NAME="paymentdate" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="The date of the payment."/>
        <FIELD NAME="usermodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="usermodified" TYPE="foreign" FIELDS="usermodified" REFTABLE="user" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="gmk_orders" COMMENT="This table stores all orders related to the different products or payable items in the platform.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="THe ID of the order is related with."/>
        <FIELD NAME="oid" TYPE="char" LENGTH="30" NOTNULL="false" SEQUENCE="false" COMMENT="The ID of the order that will be sent to the payment method, and then validated when the callback is called."/>
        <FIELD NAME="itemtype" TYPE="char" LENGTH="50" NOTNULL="false" SEQUENCE="false" COMMENT="The type of payable item. Ex: tuition, credit, contract_fee...."/>
        <FIELD NAME="itemname" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="paymentmethod" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="status" TYPE="char" LENGTH="30" NOTNULL="false" DEFAULT="PENDING" SEQUENCE="false" COMMENT="The status of the order: Ex - PENDING, APPROVED, SUCCESS...."/>
        <FIELD NAME="amount" TYPE="number" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" DECIMALS="2" COMMENT="Amount to pay, or payed."/>
        <FIELD NAME="fail_reason" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="usermodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="usermodified" TYPE="foreign" FIELDS="usermodified" REFTABLE="user" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="gmk_class" COMMENT="This table contains the classes used by the learning plans">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="name" TYPE="char" LENGTH="128" NOTNULL="true" SEQUENCE="false" COMMENT="name of the class"/>
        <FIELD NAME="type" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" COMMENT="type of the class, either inplace(0) or virtual(1)"/>
        <FIELD NAME="learningplanid" TYPE="int" LENGTH="4" NOTNULL="true" SEQUENCE="false" COMMENT="Id of the learning plan"/>
        <FIELD NAME="periodid" TYPE="int" LENGTH="4" NOTNULL="true" SEQUENCE="false" COMMENT="Id of the period"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="4" NOTNULL="true" SEQUENCE="false" COMMENT="Id of the course"/>
        <FIELD NAME="instructorid" TYPE="int" LENGTH="4" NOTNULL="true" SEQUENCE="false" COMMENT="Id of the instructor"/>
        <FIELD NAME="inittime" TYPE="char" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Init time of the class"/>
        <FIELD NAME="endtime" TYPE="char" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="End time of the class"/>
        <FIELD NAME="initdate" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="Start date of the class period"/>
        <FIELD NAME="enddate" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="End date of the class period"/>
        <FIELD NAME="classdays" TYPE="char" LENGTH="13" NOTNULL="true" SEQUENCE="false" COMMENT="Days when the class is dictated, the format is L/M/M/J/V/S/D, and the values that take each day is between 0 and 1, 0 for no dictated, 1 for dictated"/>
        <FIELD NAME="usermodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="groupid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="The id of the group created for the class"/>
        <FIELD NAME="coursesectionid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="The section id of the course created for the class"/>
        <FIELD NAME="classroomid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="classroomcapacity" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="40" SEQUENCE="false"/>
        <FIELD NAME="typelabel" TYPE="char" LENGTH="15" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="corecourseid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="instructorlpid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="inithourformatted" TYPE="char" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="endhourformatted" TYPE="char" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="inittimets" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="endtimets" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="classduration" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="approved" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="closed" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="subperiodid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="attendancemoduleid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="bbbmoduleids" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="gradecategoryid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="usermodified" TYPE="foreign" FIELDS="usermodified" REFTABLE="user" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
