<div class="modal fade" id="cancelcontractModalLong" tabindex="-1" role="dialog" aria-labelledby="cancelcontractModalLongTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header justify-content-center">
                <h5 class="modal-title position-absolute">{{#str}}cancel_contract, local_grupomakro_core {{/str}}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body text-center">
                {{#str}}msncancel, local_grupomakro_core {{/str}}
            </div>
            
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">{{#str}}cancel, local_grupomakro_core {{/str}}</button>
                <a href="{{cancelurl}}" class="btn btn-primary">{{#str}}accept, local_grupomakro_core {{/str}}</a>
            </div>
        </div>
    </div>
</div>